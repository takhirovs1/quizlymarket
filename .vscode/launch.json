  "version": "0.2.0",
  "configurations": [
    {
      "name": "quizlymarket",
      "request": "launch",
      "type": "dart",
      "noDebug": true,
      "args": ["--dart-define-from-file=config/development.json"]
    },
    {
      "name": "quizlymarket (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--dart-define-from-file=config/staging.json"]
    },
    {
      "name": "quizlymarket (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--dart-define-from-file=config/production.json"]
    }
  ]
}
